{
	"compilerOptions": {
		"types": ["svelte","vite/client"],
		"target": "esnext",
		"module": "esnext",
		"strict": true,
		"alwaysStrict": true,
		"importsNotUsedAsValues": "error",
		"isolatedModules": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"resolveJsonModule": true
	},
	"include": ["src/**/*.d.ts", "src/**/*.ts", "src/**/*.js", "src/**/*.svelte"]
}
